<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY pronto "http://clarkparsia.com/pronto#" >
    <!ENTITY owl11 "http://www.w3.org/2006/12/owl11#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl11xml "http://www.w3.org/2006/12/owl11-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY test "file:test_prefix" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="file:test_prefix#"
     xml:base="file:test_prefix"
     xmlns:owl11="http://www.w3.org/2006/12/owl11#"
     xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:pronto="http://clarkparsia.com/pronto#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:test="file:test_prefix/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="file:test_data/zpartition/test_zp_1.xml">
        <rdfs:label>Test</rdfs:label>
        <rdfs:comment
            >First very simple probabilistic part of probabilistic ontology</rdfs:comment>
        <owl:versionInfo>1.0</owl:versionInfo>
        <owl:imports rdf:resource="file:test_data/zpartition/test.xml"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&pronto;certainty"/>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&test;#Bird"/>    
    <owl:Class rdf:about="&test;#FlyingObject"/>
    <owl:Class rdf:about="&test;#WingedObject"/>
    <owl:Class rdf:about="&test;#Penguin"/>
    <owl:Thing rdf:about="&test;#Tweety"/>
    <owl:Thing rdf:about="&test;#Oscar"/>


    <owl11:Axiom>
        <rdf:subject rdf:resource="&test;#Tweety"/>
        <rdf:predicate rdf:resource="&rdf;type"/>
        <rdf:object rdf:resource="&test;#Bird"/>
        <pronto:certainty>0.8;0.9</pronto:certainty>
    </owl11:Axiom>

    <owl11:Axiom>
        <rdf:subject rdf:resource="&test;#Oscar"/>
        <rdf:predicate rdf:resource="&rdf;type"/>
        <rdf:object rdf:resource="&test;#Penguin"/>
        <pronto:certainty>0.99;1</pronto:certainty>
    </owl11:Axiom>

    <owl11:Axiom>
        <rdf:subject rdf:resource="&test;#Bird"/>
        <rdf:predicate rdf:resource="&rdfs;subClassOf"/>
        <rdf:object rdf:resource="&test;#FlyingObject"/>
        <pronto:certainty>0.9;0.95</pronto:certainty>
    </owl11:Axiom>
    <owl11:Axiom>
        <rdf:subject rdf:resource="&test;#Bird"/>
        <rdf:predicate rdf:resource="&rdfs;subClassOf"/>
        <rdf:object rdf:resource="&test;#WingedObject"/>
        <pronto:certainty>0.95;1</pronto:certainty>
    </owl11:Axiom>
    <owl11:Axiom>
        <rdf:subject rdf:resource="&test;#Penguin"/>
        <rdf:predicate rdf:resource="&rdfs;subClassOf"/>
        <rdf:object rdf:resource="&test;#FlyingObject"/>
        <pronto:certainty>0;0.05</pronto:certainty>
    </owl11:Axiom>


</rdf:RDF>
